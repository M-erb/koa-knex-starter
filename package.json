{
  "name": "koa-knex-starter",
  "version": "1.0.0",
  "description": "EWD admin area",
  "main": "server/server.js",
  "type": "module",
  "scripts": {
    "start": "node server/server.js",
    "pm2-start": "pm2 start ecosystem.config.cjs",
    "pm2-stop": "pm2 stop ecosystem.config.cjs",
    "pm2-restart": "pm2 restart ecosystem.config.js",
    "dev": "supervisor -i client,public,.data -e node,js,njk,json server/server.js",
    "dev-console": "supervisor -i client,public,.data -e node,js,njk,json --inspect server/server.js",
    "deploy": "node ./commands/deploy.js",
    "knex": "knex",
    "migrate-make": "knex migrate:make",
    "migrate-create": "knex migrate:make",
    "migrate-latest": "knex migrate:latest",
    "migrate-rollback": "knex migrate:rollback",
    "migrate-up": "knex migrate:up",
    "migrate-down": "knex migrate:down",
    "migrate-list": "knex migrate:list"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "volta": {
    "node": "18.14.2",
    "npm": "9.5.1"
  },
  "dependencies": {
    "@koa/cors": "^4.0.0",
    "@koa/router": "^12.0.0",
    "argon2": "^0.30.3",
    "chalk": "^5.2.0",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "dotenv-cast": "^1.0.1",
    "fs-extra": "^11.1.1",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.4.2",
    "koa": "^2.14.1",
    "koa-body": "^6.0.1",
    "koa-helmet": "^7.0.1",
    "koa-qs": "^3.0.0",
    "koa-static": "^5.0.0",
    "mysql2": "^3.2.0",
    "nanoid": "^4.0.1",
    "nodemailer": "^6.9.1",
    "sanitize-html": "^2.10.0",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "eslint": "^8.36.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "inquirer": "^9.1.5",
    "node-notifier": "^10.0.1",
    "supervisor": "^0.12.0"
  }
}
